<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity2">

    <View
            android:layout_width="wrap_content"
            android:layout_height="80dp"
            android:background="#BDBDBD"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/view"/>
    <ImageButton
            android:src="@drawable/baseline_local_grocery_store_24"
            android:layout_width="wrap_content"
            android:elevation="90dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="150dp"
            android:backgroundTint="#00000000"
            android:clickable="true"
            android:id="@+id/floatingActionButton2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="KeyboardInaccessibleWidget"
            app:layout_constraintTop_toTopOf="@+id/view"/>

    <ImageButton
            android:src="@drawable/baseline_fastfood_24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:backgroundTint="#00000000"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="KeyboardInaccessibleWidget"
            app:layout_constraintTop_toTopOf="@+id/view" android:id="@+id/imageButton2"/>

    <ImageButton
            android:src="@drawable/baseline_home_24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:backgroundTint="#00C42C2C"
            android:layout_marginEnd="300dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="KeyboardInaccessibleWidget" app:layout_constraintTop_toTopOf="@+id/view"/>

    <ImageButton
            android:src="@drawable/baseline_person_outline_24"
            android:layout_width="wrap_content"
            android:id="@+id/ProfileBtn"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:layout_marginStart="300dp"
            android:backgroundTint="#00000000"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="KeyboardInaccessibleWidget"
            app:layout_constraintTop_toTopOf="@+id/view"/>

    <ImageButton
            android:src="@drawable/baseline_cake_24"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:clickable="true"
            android:backgroundTint="#00000000"
            android:layout_marginStart="150dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="KeyboardInaccessibleWidget" app:layout_constraintTop_toTopOf="@+id/view"/>


</androidx.constraintlayout.widget.ConstraintLayout>